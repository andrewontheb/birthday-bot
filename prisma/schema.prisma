generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Birthday {
  id        Int      @id @default(autoincrement())
  userId    String   @unique
  chatId    BigInt
  date      DateTime
  createdAt DateTime @default(now())
}

model Chat {
  id    BigInt    @id
  title String
}
